@article{scho18,
  title = {The {{Dance}} of the {{Mechanisms}}: {{How Observed Information Influences}} the {{Validity}} of {{Missingness Assumptions}}},
  shorttitle = {The {{Dance}} of the {{Mechanisms}}},
  author = {Schouten, Rianne Margaretha and Vink, Gerko},
  year = {2021},
